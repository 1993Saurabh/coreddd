<Project>

  <PropertyGroup>
    <Version>3.0.35</Version>
    <AssemblyVersion>3.0.0.0</AssemblyVersion>
    <FileVersion>$(Version).0</FileVersion>
    <Authors>Martin Havlišta</Authors>
    <Company></Company>
    <Product />
    <PackageProjectUrl>https://github.com/xhafan/coreddd</PackageProjectUrl>
    <PackageLicenseUrl>https://raw.githubusercontent.com/xhafan/coreddd/master/LICENSE</PackageLicenseUrl>
    <PackageReleaseNotes>Downgraded Ninject nuget reference to version 3.2.2 (from 3.3.4) for CoreDdd*Ninject* nuget packages to be able to reference Ninject.Web (which requires Ninject < 3.3.0) in ASP.NET WebForms projects.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="SourceLink.Create.CommandLine" Version="2.8.1" PrivateAssets="All" /> 
  </ItemGroup>

  <PropertyGroup Condition="'$(Configuration)'=='Debug'">
    <DebugType>full</DebugType>
    <DebugSymbols>true</DebugSymbols>
  </PropertyGroup>

</Project>