<Project>

  <PropertyGroup>
    <Version>3.0.29</Version>
    <AssemblyVersion>$(Version).0</AssemblyVersion>
    <FileVersion>$(Version).0</FileVersion>
    <Authors>Martin Havlišta</Authors>
    <Company></Company>
    <Product />
    <PackageProjectUrl>https://github.com/xhafan/coreddd</PackageProjectUrl>
    <PackageLicenseUrl>https://raw.githubusercontent.com/xhafan/coreddd/master/LICENSE</PackageLicenseUrl>
    <PackageReleaseNotes>Added projects for ASP.NET Core Dependency Injection registrations. Consolidated Castle.Windsor installers into one installer per assembly.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="SourceLink.Create.CommandLine" Version="2.8.1" PrivateAssets="All" /> 
  </ItemGroup>

  <PropertyGroup Condition="'$(Configuration)'=='Debug'">
    <DebugType>full</DebugType>
    <DebugSymbols>true</DebugSymbols>
  </PropertyGroup>

</Project>