<Project>

  <PropertyGroup>
    <Version>3.0.32</Version>
    <AssemblyVersion>3.0.0.0</AssemblyVersion>
    <FileVersion>$(Version).0</FileVersion>
    <Authors>Martin Havlišta</Authors>
    <Company></Company>
    <Product />
    <PackageProjectUrl>https://github.com/xhafan/coreddd</PackageProjectUrl>
    <PackageLicenseUrl>https://raw.githubusercontent.com/xhafan/coreddd/master/LICENSE</PackageLicenseUrl>
    <PackageReleaseNotes>Made NHibernate configuration file location (path to hibernate.cfg.xml) configurable - needed when overriding the configuration for Docker</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="SourceLink.Create.CommandLine" Version="2.8.1" PrivateAssets="All" /> 
  </ItemGroup>

  <PropertyGroup Condition="'$(Configuration)'=='Debug'">
    <DebugType>full</DebugType>
    <DebugSymbols>true</DebugSymbols>
  </PropertyGroup>

</Project>